\ADD{\quadbr}{\ntauerr}{\quadbr}
\SQRTfunction{\quadbr}{\quadbr}{\Dquadbr}
\MULTIPLY{100}{\quadbr}{\quadbr}
\ROUND[1]{\quadbr}{\quadbr}

